{
  "problemMatcher": [
    {
      "owner": "luacheck-plain_error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^[\\s]*(.+?):([0-9]+):([0-9]+|([0-9]+)-([0-9]+)):\\s*\\(([E][0-9]+)\\)\\s*(.+?)$",
          "file": 1,
          "line": 2,
          "column": 4,
          "message": 7,
          "code": 6
        }
      ]
    },
    {
      "owner": "luacheck-plain_warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^[\\s]*(.+?):([0-9]+):([0-9]+|([0-9]+)-([0-9]+)):\\s*\\(([W][0-9]+)\\)\\s*(.+?)$",
          "file": 1,
          "line": 2,
          "column": 4,
          "message": 7,
          "code": 6
        }
      ]
    }
  ]
}
